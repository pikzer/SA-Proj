<AnchorPane onMouseClicked="#handleAnchorClick" prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: CFEBF7;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CheckingApp.addCusController">
